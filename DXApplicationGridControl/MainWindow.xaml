<dx:ThemedWindow
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
    xmlns:dxg="http://schemas.devexpress.com/winfx/2008/xaml/grid" 
    x:Class="DXApplicationGridControl.MainWindow"
    Title="MainWindow" Height="800" Width="1000">
    <Grid>
        <dxg:GridControl Name="gridControl" AllowInitiallyFocusedRow="True" Margin="-613,29,613,-29">
            <dxg:GridControl.Columns>
                <dxg:GridColumn FieldName="ID" Header="ID" Width="50" />
                <dxg:GridColumn FieldName="Name" Header="Name" />
                <dxg:GridColumn FieldName="Age" Header="Age" />
            </dxg:GridControl.Columns>
        </dxg:GridControl>
    </Grid>
</dx:ThemedWindow>
